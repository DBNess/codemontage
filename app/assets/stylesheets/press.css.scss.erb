@import "colors";

#press {
  padding-bottom: 30px;
  
  #press_logos {
    a {
      height: 100px;
      background-color: transparent;
      background-repeat: no-repeat;
      background-position: 50% 50%;
      background-size: 100px auto;
      
    }
    a.skillcrush { background-image: url(<%= asset_path 'logos/skillcrush_logo.png' %>) }
    a.furthered { background-image: url(<%= asset_path 'logos/furthered_logo.jpg' %>) }
    a.todayshow { background-image: url(<%= asset_path 'logos/todayshow_logo.png' %>) }
    a.whitehouse { background-image: url(<%= asset_path 'logos/whitehouse_logo.png' %>) }
    a.aoljobs { background-image: url(<%= asset_path 'logos/aol_jobs_logo.png' %>) }
    a.opensource { background-image: url(<%= asset_path 'logos/opensource_com_logo.png' %>) }
  }
}
